
<div class="center">

<div class="row">
<% @listings.each do |listing| %>
  <div class="col-md-3">
   <div class="thumbnail">
       <%= image_tag listing.image_url%>
      <div class="caption">
       <h3> <%= listing.name %> </h3>
        <%= number_to_currency(listing.price) %>
        <p><%= "Sold by #{listing.user.name} " %></p>
        <%= link_to 'Show', listing %>
        <%= link_to 'Edit', edit_listing_path(listing) %>
        <%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %>
        
      </div>
    </div>
  
    
   </div>
<% end %>
</div>
</div>

<%= link_to 'New Listing', new_listing_path, class: "btn btn-default" %>
